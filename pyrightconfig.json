{
  "venvPath": ".venv",
  "venv": "karrio",
  "pythonVersion": "3.12",
  "pythonPlatform": "Darwin",
  "include": [
    "modules",
    "karrio",
    "insiders"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*",
    "apps/dashboard"
  ],
  "extraPaths": [
    "./karrio/modules/*",
    "./karrio/modules/connectors/*",
    "./karrio/apps/*",
    "./insiders/modules/*",
    "./insiders/modules/connectors/*",
    "./modules/*"
  ],
  "useLibraryCodeForTypes": true,
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "stubPath": ".venv/karrio/lib/python3.12/site-packages"
}
