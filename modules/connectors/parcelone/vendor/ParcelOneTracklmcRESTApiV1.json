        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "https://sandbox.parcelone.io/tracklmc",
            "description": "Parcel.One TrackLMC REST API - Sandbox"
        }
    ],
    "paths": {
        "/shipment/{trackno}": {
            "get": {
                "tags": [
                    "shipment"
                ],
                "summary": "get Tracking data for Shipment",
                "description": "get Tracking data for Shipment",
                "operationId": "getTrackings",
                "parameters": [
                    {
                        "$ref": "#/components/parameters/trackno"
                    },
                    {
                        "$ref": "#/components/parameters/lang"
                    },
                    {
                        "$ref": "#/components/parameters/platform"
                    },
                    {
                        "$ref": "#/components/parameters/requestor"
                    },
                    {
                        "$ref": "#/components/parameters/Getaccesstoken"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "allOf": [
                                        {
                                            "properties": {
                                                "P1Trackno": {
                                                    "description": "Parcel.One Tracknumber from request",
                                                    "type": "string"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        {
                                            "properties": {
                                                "TrackingEvents": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string"
                                                    }
                                                }
                                            },
                                            "type": "object"
                                        },
                                        {
                                            "properties": {
                                                "lang": {
                                                    "description": "Language parameter from request",
                                                    "type": "string"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        {
                                            "properties": {
                                                "platform": {
                                                    "description": "Platform parameter from request",
                                                    "type": "string"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        {
                                            "properties": {
                                                "requestor": {
                                                    "description": "Requestor parameter from request",
                                                    "type": "string"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    ]
                                },
                                "examples": {
                                    "ResultSuccess": {
                                        "summary": "ResultSuccess",
                                        "value": {
                                            "P1Trackno": "4050100151016",
                                            "TrackingEvents": [
                                                {
                                                    "EventdateCET": "2021-10-18T08:26:05+02:00",
                                                    "EventdateUTC": "2021-10-18T06:26:05+00:00",
                                                    "Statuscode": "P100",
                                                    "Status": "The shipment has been prepared for transportation by the sender.",
                                                    "Location": "DE Füssen",
                                                    "DeliveryStatus": "InfoReceived_001",
                                                    "CarrierTrackno": "",
                                                    "Carrier": "",
                                                    "CarrierSlug": "",
                                                    "CarrierTrackURL": "",
                                                    "TrackingEventsStatus": ""
                                                },
                                                {
                                                    "EventdateCET": "2021-10-18T09:43:18+02:00",
                                                    "EventdateUTC": "2021-10-18T07:43:18+00:00",
                                                    "Statuscode": "P101",
                                                    "Status": "The shipment has been handed over to a carrier and is now on it's way to PARCEL.ONE.",
                                                    "Location": "DE Füssen",
                                                    "DeliveryStatus": "Pending_001",
                                                    "CarrierTrackno": "",
                                                    "Carrier": "",
                                                    "CarrierSlug": "",
                                                    "CarrierTrackURL": "",
                                                    "TrackingEventsStatus": ""
                                                },
                                                {
                                                    "EventdateCET": "2021-10-20T09:01:00+02:00",
                                                    "EventdateUTC": "2021-10-20T07:01:00+00:00",
                                                    "Statuscode": "300",
                                                    "Status": "The shipment arrived the P1 HUB and is now being handled.",
                                                    "Location": "DE Pohlheim",
                                                    "DeliveryStatus": "InTransit_003",
                                                    "CarrierTrackno": "",
                                                    "Carrier": "",
                                                    "CarrierSlug": "",
                                                    "CarrierTrackURL": "",
                                                    "TrackingEventsStatus": ""
                                                },
                                                {
                                                    "EventdateCET": "2021-10-20T16:15:00+02:00",
                                                    "EventdateUTC": "2021-10-20T14:15:00+00:00",
                                                    "Statuscode": "394",
                                                    "Status": "The shipment has left the P1 HUB and can be tracked further in the tracking system of <a class=clink href=https://tools.usps.com/go/TrackConfirmAction?tLabels=LX035843846BE target=_blank>USPS</a> using the shipment number <a class=clink href=https://tools.usps.com/go/TrackConfirmAction?tLabels=LX035843846BE target=_blank>LX035843846BE</a>.",
                                                    "Location": "DE Pohlheim",
                                                    "DeliveryStatus": "InTransit_001",
                                                    "CarrierTrackno": "LX035843846BE",
                                                    "Carrier": "USPS",
                                                    "CarrierSlug": "correos-de-mexico",
                                                    "CarrierTrackURL": "https://tools.usps.com/go/TrackConfirmAction?tLabels=LX035843846BE",
                                                    "TrackingEventsStatus": "P1TrackingEndsHere,RequestLMCCarrierTracking"
                                                }
                                            ],
                                            "lang": "en",
                                            "platform": "",
                                            "requestor": ""
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Authentification failed"
                    },
                    "404": {
                        "description": "Not found",
                        "content": {
                            "application/json": {
                                "schema": {},
                                "examples": {
                                    "ResultError Shipment not found": {
                                        "summary": "ResultError Shipment not found",
                                        "value": {
                                            "Error": "No shipment data found yet, try again later!"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "parameters": {
            "lang": {
                "name": "lang",
                "in": "query",
                "description": "Setting to control multilingual response",
                "required": false,
                "schema": {
                    "type": "string",
                    "default": "en",
                    "enum": [
                        "en",
                        "de",
                        "es"
                    ]
                }
            },
            "Getaccesstoken": {
                "name": "Getaccesstoken",
                "in": "header",
                "description": "Header-flag to return JWT Access Token, insert 'true' to send this header",
                "required": false,
                "schema": {
                    "type": "string",
                    "enum": [
                        "true"
                    ]
                }
            },
            "trackno": {
                "name": "trackno",
                "in": "path",
                "description": "search Tracking data for Parcel.One-Shipment Trackingnumber - 13 digits",
                "required": true,
                "schema": {
                    "type": "string"
                }
            },
            "platform": {
                "name": "platform",
                "in": "query",
                "description": "platform (string) - maxlen 30 - if you are a platform provider, please, specify here, e.g. Ebay, Amazon, ...",
                "required": false,
                "schema": {
                    "type": "string"
                }
            },
            "requestor": {
                "name": "requestor",
                "in": "query",
                "description": "requestor (string) - maxlen 30 - your company name",
                "required": false,
                "schema": {
                    "type": "string"
                }
            }
        },
        "securitySchemes": {
            "api_key": {
                "type": "apiKey",
                "name": "Apikey",
                "in": "header"
            },
            "BasicAuth": {
                "type": "http",
                "scheme": "basic"
            },
            "Bearer_JWT": {
                "type": "http",
                "bearerFormat": "JWT",
                "scheme": "bearer"
            }
        }
    },
    "security": [
        {
            "api_key": [],
            "BasicAuth": [],
            "Bearer_JWT": []
        }
    ],
    "tags": [
        {
            "name": "Migration from WCF",
            "description": "If you are migrating from our previos WCF-API to the actual REST-API, you will find useful information about which WCF-function corresponds to which REST-API-Path e.g. in our specific external documentation!",
            "externalDocs": {
                "description": "Doc",
                "url": "https://doc.parcelone.io/ParcelOneShippingRESTApiV1-migration.html"
            }
        },
        {
            "name": "Multilinguality",
            "description": "To make use of our multilingual support, please, specify a Header named 'Lang' with a value of one of our supported languages in ISO-3166-2-format: 'de' for German, 'en' for English or 'es' for Spanish."
        }
    ],
    "externalDocs": {
        "description": "Find out more about Parcel.One",
        "url": "http://parcel.one"
    }
